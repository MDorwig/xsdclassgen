            xmlns:xs="http://www.w3.org/2001/XMLSchema"
            xmlns:ex="http://ns.espa-x.org/espa-x"
            elementFormDefault="qualified"
            targetNamespace="http://ns.tetronik.com/DAKS_ESPA-X"> 
  
  <!-- Copyright -->
  <xs:annotation>
    <xs:documentation>
      DAKS_ESPA-X Version 1.12.
      Copyright 20012 tetronik GmbH AEN.
      All rights reserved.
    </xs:documentation>
  </xs:annotation>

  <!-- Import standard ESPA-X data types -->
  <xs:import namespace="http://ns.espa-x.org/espa-x" schemaLocation="http://schema.espa-x.org/espa-x100.xsd"/>
  
  <!-- Deklaration of enumerations -->
  
  <xs:simpleType      name="enumDAKSVersions">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="1.10"/>
      <xs:enumeration value="1.11"/>
      <xs:enumeration value="1.12"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType      name="enumProcessTypes">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="Standard"/>
      <xs:enumeration value="Conference"/>
      <xs:enumeration value="Emergency-Announcement"/>
      <xs:enumeration value="SGL_RecPlay"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="enumBusyActions">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="Nothing"/>
      <xs:enumeration value="Camp-on"/>
      <xs:enumeration value="Emerg-Disc"/>
      <xs:enumeration value="Intrusion"/>
      <xs:enumeration value="Emerg-Intr"/>
    </xs:restriction>
  </xs:simpleType>
	
  <xs:simpleType      name="enumCnctCtrlMemberBusyActions">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="Default"/>
      <xs:enumeration value="CampOn"/>
      <xs:enumeration value="Intr"/>
      <xs:enumeration value="EmergencyIntr"/>
      <xs:enumeration value="ForcedRel"/>
    </xs:restriction>
  </xs:simpleType>
	
  <xs:simpleType      name="enumCnctCtrlTrunkBusyActions">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="Default"/>
      <xs:enumeration value="EmergencyIntr"/>
			<xs:enumeration value="ForcedRel"/>
    </xs:restriction>
  </xs:simpleType>
	

  <xs:simpleType      name="enumKeyIndications">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="No"/>
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
      <xs:enumeration value="4"/>
      <xs:enumeration value="5"/>
      <xs:enumeration value="6"/>
      <xs:enumeration value="7"/>
      <xs:enumeration value="8"/>
      <xs:enumeration value="9"/>
      <xs:enumeration value="*" id="star"/>
      <xs:enumeration value="#" id="hash"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="enumKeyIndicationsEx">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="No"/>
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
      <xs:enumeration value="4"/>
      <xs:enumeration value="5"/>
      <xs:enumeration value="6"/>
      <xs:enumeration value="7"/>
      <xs:enumeration value="8"/>
      <xs:enumeration value="9"/>
      <xs:enumeration value="*" />
      <xs:enumeration value="#" />
      <xs:enumeration value="Off"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="enumMLPPLevels">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="Routine"/>
      <xs:enumeration value="Priority"/>
      <xs:enumeration value="Immediate"/>
      <xs:enumeration value="Flash"/>
      <xs:enumeration value="Flash Override"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType      name="enumConfirmations">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="None"/>
      <xs:enumeration value="Positive"/>
      <xs:enumeration value="Negative"/>
      <xs:enumeration value="Wrong Person"/>
      <xs:enumeration value="Callback"/>
      <xs:enumeration value="Positive+Time"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="enumRecPlayControls">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="None"/>
      <xs:enumeration value="Record"/>
      <xs:enumeration value="Record End"/>
      <xs:enumeration value="Record OK"/>
      <xs:enumeration value="Play"/>
      <xs:enumeration value="Play End"/>
      <xs:enumeration value="RecPlay End"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType      name="enumMemberType">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="Operator"/>
      <xs:enumeration value="Master"/>
      <xs:enumeration value="Standard"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="enumSpeakerPhoneControls">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="No"/>
      <xs:enumeration value="Yes"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="enumPinNeutral">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="No"/>
      <xs:enumeration value="Yes"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="enumAutoCallbacks">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="No"/>
      <xs:enumeration value="Yes"/>
      <xs:enumeration value="SPVC"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="enumValidChars">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="Any"/>
      <xs:enumeration value="Numeric"/>
      <xs:enumeration value="Keypad"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType      name="enumCallHandles">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="Update"/>
      <xs:enumeration value="Dial"/>
      <xs:enumeration value="Alerting"/>
      <xs:enumeration value="Accept"/>
      <xs:enumeration value="Busy"/>
      <xs:enumeration value="Invalid"/>
      <xs:enumeration value="Disconnect"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="enumCallType">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="Voice"/>
      <xs:enumeration value="Data"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="enumCallStatus">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="Timedout"/>
      <xs:enumeration value="Setup"/>
      <xs:enumeration value="Ringing"/>
      <xs:enumeration value="Busy"/>
      <xs:enumeration value="Invalid"/>
      <xs:enumeration value="NotResponded"/>
      <xs:enumeration value="Connected"/>
      <xs:enumeration value="Consulting"/>
      <xs:enumeration value="Returned"/>
      <xs:enumeration value="Transferred"/>
      <xs:enumeration value="Conferenced"/>
      <xs:enumeration value="Closed"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType      name="enumDialogStatus">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="Started"/>  
      <xs:enumeration value="Input"/>  
      <xs:enumeration value="Ended"/>  
      <xs:enumeration value="Cancelled"/>  
      <xs:enumeration value="Timeout"/>  
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="enumDialogInfos">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="Standard"/>
      <xs:enumeration value="Enchanced"/>
      <xs:enumeration value="All"/>
    </xs:restriction>
  </xs:simpleType>  
  
  <xs:simpleType      name="enumControlNotify">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="Enabled"/>
      <xs:enumeration value="Disabled"/>
    </xs:restriction>
  </xs:simpleType>  
  
  <xs:simpleType      name="enumInfotelProfiles">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="Off"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
      <xs:enumeration value="4"/>
      <xs:enumeration value="5"/>
      <xs:enumeration value="6"/>
      <xs:enumeration value="7"/>
      <xs:enumeration value="8"/>
      <xs:enumeration value="9"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType      name="enumMbrCtrlCommand">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="CANCELCALL"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="strSingleChar">
    <xs:restriction   base="xs:string">
      <xs:minLength   value="0" />
      <xs:maxLength   value="1"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="strIdentifier">
    <xs:restriction   base="xs:string">
      <xs:minLength   value="1" />
      <xs:maxLength   value="4"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="strCtrlIdentifier">
    <xs:restriction   base="xs:string">
      <xs:minLength   value="1" />
      <xs:maxLength   value="10"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="strDialogRef">
    <xs:restriction   base="xs:string">
      <xs:minLength   value="1" />
      <xs:maxLength   value="8"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType      name="strTitle">
    <xs:restriction   base="xs:string">
      <xs:minLength   value="1" />
      <xs:maxLength   value="10"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType      name="strLongTitle">
    <xs:restriction   base="xs:string">
      <xs:minLength   value="1" />
      <xs:maxLength   value="16"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="strSecurityCode">
    <xs:restriction   base="xs:string">
      <xs:minLength   value="1" />
      <xs:maxLength   value="16"/>
      <xs:pattern     value="[0-9]+"/>
    </xs:restriction>
  </xs:simpleType>

  <!--  
  <xs:simpleType      name="strResourceName">
    <xs:restriction   base="xs:string">
      <xs:minLength   value="1" />
      <xs:maxLength   value="24"/>
    </xs:restriction>
  </xs:simpleType>
 -->
  <xs:simpleType      name="strURL">
    <xs:restriction   base="xs:string">
      <xs:minLength   value="1" />
      <xs:maxLength   value="1024"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="strConType">
    <xs:restriction   base="xs:string">
      <xs:minLength   value="1" />
      <xs:maxLength   value="3"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType     name="strAnnData">
    <xs:restriction   base="xs:string">
      <xs:minLength   value="0" />
      <xs:maxLength   value="64000"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType    name="strTTVAnnData">
    <xs:restriction  base="xs:string">
      <xs:minLength   value="0" />
      <xs:maxLength   value="16348"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType  name="strPlayDuration">
    <xs:restriction  base="xs:string">
      <xs:pattern     value="[0-9]{1,4}[sc]{1}"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType      name="strNumericKey">
    <xs:restriction   base="xs:string">
      <xs:minLength   value="1" />
      <xs:maxLength   value="1"/>
      <xs:pattern     value="[0-9]"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType  name="strAccessID">
    <xs:restriction  base="xs:string">
      <xs:minLength   value="1" />
      <xs:maxLength   value="4"/>
      <xs:pattern     value="[0-9]+"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType  name="strPIN">
    <xs:restriction  base="xs:string">
      <xs:minLength   value="1" />
      <xs:maxLength   value="6"/>
      <xs:pattern     value="[0-9]+"/>
    </xs:restriction>
  </xs:simpleType>


  <xs:simpleType     name="uiDialThru">
    <xs:restriction   base="xs:unsignedInt">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="3"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType     name="uiNoOfWaitpos">
    <xs:restriction   base="xs:unsignedInt">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="99"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType     name="uiTTVLanguage">
    <xs:restriction   base="xs:unsignedInt">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="16"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType     name="uiTTVVoice">
    <xs:restriction   base="xs:unsignedInt">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="1"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType     name="uiEditLength">
    <xs:restriction   base="xs:unsignedInt">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="256"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType     name="uiPAChannel">
    <xs:restriction   base="xs:unsignedInt">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="8"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- Type deklaration for PA channel list  -->

  <xs:simpleType      name="listPAChannels">
    <xs:list          itemType="dx:uiPAChannel"/>
  </xs:simpleType>

  <xs:simpleType      name="typePAChannels">
    <xs:restriction   base="dx:listPAChannels">
      <xs:minLength   value="1"/>
      <xs:maxLength   value="8"/>
    </xs:restriction>
  </xs:simpleType>


  <!-- Type deklaration for callback functionality in prepared calls -->
  
  <xs:complexType     name="typePrepCallback">
    <xs:sequence>
      <xs:element     name="DIALTHRU"         type="dx:uiDialThru"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="NO_OF_WAITPOS"    type="dx:uiNoOfWaitpos"
                                              minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>
  
  <!-- Type deklaration for list of DTMF and Keypad evaluation  -->
  <xs:simpleType      name="enumIgnoreKeys">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="RTP"/>
      <xs:enumeration value="DTMF"/>
      <xs:enumeration value="Keypad"/>
    </xs:restriction>
  </xs:simpleType>  

  <xs:simpleType    name="listIgnoreKeys">
    <xs:list          itemType="dx:enumIgnoreKeys"/>
  </xs:simpleType>

  <xs:simpleType      name="typeIgnoreKeys">
    <xs:restriction   base="dx:listIgnoreKeys">
      <xs:minLength   value="1"/>
      <xs:maxLength   value="3"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- Type deklaration for list of phone keys  -->
  <xs:simpleType    name="listKeys">
    <xs:list          itemType="ex:strKeyCode"/>
  </xs:simpleType>

  <xs:simpleType      name="typeKeyList">
    <xs:restriction   base="dx:listKeys">
      <xs:minLength   value="1"/>
      <xs:maxLength   value="12"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- Type deklaration for list of announcements  -->
  
  <xs:simpleType      name="listAnnouncements">
    <xs:list          itemType="dx:strIdentifier"/>
  </xs:simpleType>

  <xs:simpleType      name="typeAnnouncements">
    <xs:restriction   base="dx:listAnnouncements">
      <xs:minLength   value="1"/>
      <xs:maxLength   value="16"/>
    </xs:restriction>
  </xs:simpleType>
 
  <!-- Type deklaration for endless list of announcements  -->
  
  <xs:complexType     name="typeAnnList">
    <xs:sequence>
      <xs:element     name="SA-ANNID"         type="dx:strIdentifier"
                                              minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <!-- Type deklaration for endless list of groups  -->
  
  <xs:complexType     name="typeGroupList">
    <xs:sequence>
      <xs:element     name="SG-GROUPID"       type="dx:strIdentifier"
                                              minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <!-- Type deklaration for endless list of members  -->
  <xs:complexType     name="typeMemberList">
    <xs:sequence>
      <xs:element     name="SS-SBSCRID"       type="ex:strObjectId"
                                              minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
   
   <!-- Type deklaration for audio transmission -->
  
  <xs:complexType     name="typeAnnData">
    <xs:simpleContent>
      <xs:extension   base="dx:strAnnData">
        <xs:attribute     name="segNo"            type="xs:unsignedLong"
                                                  use="required"/>
        <xs:attribute     name="isLast"           type="xs:boolean"
                                                  use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  
  <xs:complexType     name="typeKeyConfirmations">
    <xs:simpleContent>
      <xs:extension   base="dx:enumConfirmations">
        <xs:attribute     name="title"            type="dx:strTitle"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType     name="typeRecPlayControls">
    <xs:simpleContent>
      <xs:extension   base="dx:enumRecPlayControls">
        <xs:attribute     name="title"            type="dx:strTitle"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType      name="enumDelRpAnn">
    <xs:restriction   base="xs:string">
      <xs:enumeration value="No"/>
      <xs:enumeration value="Yes"/>
    </xs:restriction>
  </xs:simpleType>
  
  <!-- Type deklaration for announcement handling  -->

  <xs:complexType     name="typeRecPlay">
    <xs:all>
      <xs:element     name="KEY_0"            type="dx:typeRecPlayControls"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_1"            type="dx:typeRecPlayControls"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_2"            type="dx:typeRecPlayControls"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_3"            type="dx:typeRecPlayControls"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_4"            type="dx:typeRecPlayControls"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_5"            type="dx:typeRecPlayControls"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_6"            type="dx:typeRecPlayControls"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_7"            type="dx:typeRecPlayControls"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_8"            type="dx:typeRecPlayControls"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_9"            type="dx:typeRecPlayControls"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="RECPLAY_ANN"      type="dx:strIdentifier"
                                              minOccurs="1" maxOccurs="1"/>
      <xs:element     name="REC_TEXT"         type="ex:strClipName"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="PLAY_TEXT"        type="ex:strClipName"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="DEL_RP_ANN"       type="dx:enumDelRpAnn"
                                              minOccurs="0" maxOccurs="1"/>

    </xs:all>
  </xs:complexType>


  <!-- Type deklaration for confirmation of calls  -->
  
  <xs:complexType     name="typeConfirmation">
    <xs:all>
      <xs:element     name="KEY_0"            type="dx:typeKeyConfirmations"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_1"            type="dx:typeKeyConfirmations"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_2"            type="dx:typeKeyConfirmations"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_3"            type="dx:typeKeyConfirmations"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_4"            type="dx:typeKeyConfirmations"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_5"            type="dx:typeKeyConfirmations"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_6"            type="dx:typeKeyConfirmations"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_7"            type="dx:typeKeyConfirmations"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_8"            type="dx:typeKeyConfirmations"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="KEY_9"            type="dx:typeKeyConfirmations"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="CFRM_ANN"         type="dx:strIdentifier"
                                              minOccurs="0" maxOccurs="1"/>
      <xs:element     name="CFRM_TEXT"        type="ex:strClipName"
                                              minOccurs="0" maxOccurs="1"/>
    </xs:all>
  </xs:complexType>

  <!-- Type deklaration for dialog controls -->
  
  <xs:complexType     name="typeButton">
    <xs:attribute     name="id"               type="dx:strCtrlIdentifier"
                                              use="required"/>
    <xs:attribute     name="title"            type="dx:strTitle"/>
    <xs:attribute     name="notify"           type="dx:enumControlNotify"/>
    <xs:attribute     name="keyCode"          type="dx:strNumericKey"/>
    <xs:attribute     name="annID"            type="dx:strIdentifier"/>
    <xs:attribute     name="stopAnn"          type="xs:boolean"/>
    <xs:attribute     name="closeDlg"         type="xs:boolean"/>
