							</xs:annotation>                        			                            
                        			                            </xs:element>
                        
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="oaLocationRequest">
		<xs:sequence/>
	</xs:complexType>
	<xs:complexType name="oaLocationResponse">
