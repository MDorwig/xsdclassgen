	<xs:complexType name="oaInteractiveMessage">
		<xs:sequence>
			<xs:element name="Message" type="oaMessage"/>
			<xs:element minOccurs="0" name="Update">
				<xs:annotation>
					<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Update previous sent IM</xs:documentation>
					
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="Method">
							<xs:annotation>
								<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Defines how the message update shall be handled</xs:documentation>
								
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="onlyIfNoExist"/>
									<xs:enumeration value="onlyExisting"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element minOccurs="0" name="Request_status">
