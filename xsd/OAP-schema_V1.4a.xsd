											<xs:sequence>
												<xs:element minOccurs="0" name="Method">
													<xs:annotation>
														<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Defines which method to use for the call</xs:documentation>
														
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="first"/>
															<xs:enumeration value="once"/>
															<xs:enumeration value="more"/>
															<xs:enumeration value="last"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element minOccurs="0" name="Microphone_mode">
													<xs:annotation>
														<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Defines how the microphone shall operate during the call</xs:documentation>
														
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="PTT"/>
															<xs:enumeration value="micOn"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Digits">
													<xs:annotation>
														<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Defines the call number to be dialled</xs:documentation>
														
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="32"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element minOccurs="0" name="Call_disconnect_data" type="elemIMDataResp">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Send a response when the call is disconnected</xs:documentation>
											
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="New_time_to_live">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Update time to live</xs:documentation>
											
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:integer">
												<xs:minInclusive value="1"/>
												<xs:maxInclusive value="999999"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element minOccurs="0" name="New_priority">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Change message priority</xs:documentation>
											
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:integer">
												<xs:minInclusive value="1"/>
												<xs:maxInclusive value="9"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element minOccurs="0" name="Show_next_layer">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Show next layer of options on selection</xs:documentation>
											
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:integer">
												<xs:minInclusive value="1"/>
												<xs:maxInclusive value="99"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element minOccurs="0" name="Sticky_mode" type="elemStickyMode">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Defines sticky mode for the message</xs:documentation>
											
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="Close_message">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Close the currently displayed message</xs:documentation>
											
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="Erase_message">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Erase the message on selection</xs:documentation>
											
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="Erase_option" type="elemIMoptID">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Erase an option of the interactive message on selection</xs:documentation>
											
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="Disable_option">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Disable the option after selection</xs:documentation>
											
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="Enable_option" type="elemIMoptID">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Enable disabled option(s) on selection</xs:documentation>
											
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="Mark_option">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">The option will be marked after selection</xs:documentation>
											
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="Unmark_option" type="elemIMoptID">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Unmark previously marked option(s) on selection</xs:documentation>
											
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="Data_response_address" type="elemUniteAddress">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Specifies the Unite address that should be used for data response</xs:documentation>
											
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="Call_request">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Requests number from system to use for call connections</xs:documentation>
											
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="Disconnect_call"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="oaInteractiveMessageResponse">
		<xs:sequence>
			<xs:element minOccurs="0" name="Data_response" type="elemIMDataResp">
				<xs:annotation>
					<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Predefined data that is sent as a response on selection</xs:documentation>
					
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="User_response" type="elemIMDataResp">
				<xs:annotation>
					<xs:documentation xmlns="http://www.w3.org/1999/xhtml">The data manually entered by the user after selecting an IM option</xs:documentation>
					
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="Priority" type="elemPagingPrio">
				<xs:annotation>
					<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Message priority</xs:documentation>
					
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="Update_status">
				<xs:annotation>
					<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Defines how the update of an IM was carried through</xs:documentation>
					
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="failed"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
