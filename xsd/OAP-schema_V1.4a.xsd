	<xs:complexType name="oaMessage">
		<xs:sequence>
			<xs:element minOccurs="0" name="Subject">
				<xs:annotation>
					<xs:documentation xmlns="http://www.w3.org/1999/xhtml">The subject of the message</xs:documentation>
					
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="Body">
				<xs:annotation>
					<xs:documentation xmlns="http://www.w3.org/1999/xhtml">The body of the message</xs:documentation>
					
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="5000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="Indication">
				<xs:annotation>
					<xs:documentation>The indication element  brings together a group of elements defining how to indicate the message for a user.</xs:documentation>
					
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" name="Beep">
							<xs:annotation>
								<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Beep characteristics</xs:documentation>
								
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:minInclusive value="0"/>
									<xs:maxInclusive value="99"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element minOccurs="0" name="Intensity">
							<xs:annotation>
								<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Defines the intensity of the indication</xs:documentation>
								
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Normal"/>
									<xs:enumeration value="None"/>
									<xs:enumeration value="Silent"/>
									<xs:enumeration value="Low"/>
									<xs:enumeration value="Medium"/>
									<xs:enumeration value="High"/>
									<xs:enumeration value="Max"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element minOccurs="0" name="Urgent">
							<xs:annotation>
								<xs:documentation xmlns="http://www.w3.org/1999/xhtml">The message is defined as urgent and will break through silent mode</xs:documentation>
								
							</xs:annotation>
						</xs:element>
						<xs:element minOccurs="0" name="Colour">
							<xs:annotation>
								<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Defines the colour of the indication</xs:documentation>
								
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Red"/>
									<xs:enumeration value="Green"/>
									<xs:enumeration value="Blue"/>
									<xs:enumeration value="Yellow"/>
									<xs:enumeration value="Orange"/>
									<xs:enumeration value="Cyan"/>
									<xs:enumeration value="Purple"/>
									<xs:enumeration value="Brown"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element minOccurs="0" name="Repetition">
							<xs:annotation>
								<xs:documentation>The Repetition element  brings together a group of elements defining repetition of indication (Beep) for the device</xs:documentation>
								
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Interval_time">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Interval time for repetition of indication</xs:documentation>
											
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:integer">
												<xs:minInclusive value="1"/>
												<xs:maxInclusive value="999"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="Number_of_repetitions">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Number of repetitions of indications with the interval time as set byInterval_time</xs:documentation>
											
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:integer">
												<xs:minInclusive value="1"/>
												<xs:maxInclusive value="999"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="TTL">
				<xs:annotation>
					<xs:documentation xmlns="http://www.w3.org/1999/xhtml">This is the total time the paging is valid in seconds after it has arrived to the device</xs:documentation>
					
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="999999"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="Priority" type="elemPagingPrio">
			</xs:element>
			<xs:element minOccurs="0" name="Tag">
				<xs:annotation>
					<xs:documentation xmlns="http://www.w3.org/1999/xhtml">A text that categorises the sent block</xs:documentation>
					
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="25"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="oaPagingConfirmation">
