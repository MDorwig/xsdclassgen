						<xs:element minOccurs="0" name="Function_key_ID">
							<xs:annotation>
								<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Function key for option</xs:documentation>
								
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element minOccurs="0" name="Call_request_number">
							<xs:annotation>
								<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Number identifying which layer the option belongs to</xs:documentation>
								
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="32"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element minOccurs="0" name="On_selection">
							<xs:annotation>
								<xs:documentation xmlns="http://www.w3.org/1999/xhtml">This is a subgroup to Option</xs:documentation>
								
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element minOccurs="0" name="Data_response" type="elemIMDataResp">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Data response</xs:documentation>
											
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="User_response">
										<xs:annotation>
											<xs:documentation xmlns="http://www.w3.org/1999/xhtml">This is a subgroup to On_selection</xs:documentation>
											
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element minOccurs="0" name="Data_type">
													<xs:annotation>
														<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Expected type of data in response</xs:documentation>
														
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="numeric"/>
															<xs:enumeration value="alphanum"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Text_label">
													<xs:annotation>
														<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Text prompt i.e. predefined text displayed when data shall be entered</xs:documentation>
														
													</xs:annotation>
