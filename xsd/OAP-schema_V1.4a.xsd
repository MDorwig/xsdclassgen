					<xs:sequence>
						<xs:element minOccurs="0" name="Attention">
							<xs:annotation>
								<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Defines the interval between indications for the message</xs:documentation>
								
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:minInclusive value="1"/>
									<xs:maxInclusive value="255"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element minOccurs="0" name="Session">
							<xs:annotation>
								<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Defines how long time after an option in the message has been selected until the message is deleted</xs:documentation>
								
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:minInclusive value="1"/>
									<xs:maxInclusive value="255"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="Sticky_mode" type="elemStickyMode">
				<xs:annotation>
					<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Defines sticky mode for the message</xs:documentation>
					
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="Option">
				<xs:annotation>
					<xs:documentation xmlns="http://www.w3.org/1999/xhtml">The option group contains elements relevant to user response selection</xs:documentation>
					
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0">
						<xs:element minOccurs="0" name="ID" type="elemIMoptID">
							<xs:annotation>
								<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Identification number for the option</xs:documentation>
								
							</xs:annotation>
						</xs:element>
						<xs:element minOccurs="0" name="List_order">
							<xs:annotation>
								<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Order number in list of options</xs:documentation>
								
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:minInclusive value="1"/>
									<xs:maxInclusive value="99"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element minOccurs="0" name="Display_layer">
							<xs:annotation>
								<xs:documentation xmlns="http://www.w3.org/1999/xhtml">Number identifying which layer the option belongs to</xs:documentation>
								
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:minInclusive value="1"/>
									<xs:maxInclusive value="99"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
